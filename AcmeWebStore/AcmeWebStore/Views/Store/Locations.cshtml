@using Library.Model;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
   

<html>
<body>

    <table>
        <tr>
            <th>Location </th>
            <th>Address</th>
            <th> City</th>
            <th>State</th>
            <th>Country</th>
        </tr>
        @foreach (Location x in ViewData["Locations"] as List<Location>)
        {
    <tr>
        <td>
            @x.Address
        </td>
        <td>
           
            @x.City
        </td>
        <td>
            @x.State
        </td>
        <td>
            @x.Country
        </td>
        <td>
            <a href="Store/Locations/Inventory/@x.Id">Products</a> 
        </td>
    </tr>
        }
    </table>
   
</body>
</html>